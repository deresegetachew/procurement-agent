{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "workers": {
      "type": "application",
      "root": "apps/workers",
      "entryFile": "main",
      "sourceRoot": "apps/workers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/workers/tsconfig.app.json"
      }
    },
    "db": {
      "type": "library",
      "root": "libs/db",
      "entryFile": "index",
      "sourceRoot": "libs/db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db/tsconfig.lib.json"
      }
    },
    "agents": {
      "type": "library",
      "root": "libs/agents",
      "entryFile": "index",
      "sourceRoot": "libs/agents/src",
      "compilerOptions": {
        "tsConfigPath": "libs/agents/tsconfig.lib.json"
      }
    },
    "prompts": {
      "type": "library",
      "root": "libs/prompts",
      "entryFile": "index",
      "sourceRoot": "libs/prompts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prompts/tsconfig.lib.json"
      }
    },
    "tenants": {
      "type": "library",
      "root": "libs/tenants",
      "entryFile": "index",
      "sourceRoot": "libs/tenants/src",
      "compilerOptions": {
        "tsConfigPath": "libs/tenants/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}